/*Seleção 1*/
$color1 = #282c34;
$color2 = #7f5900;
$color3 = #7f6526;
$color4 = #cc8f00;
$color5 = #F3B300;
$color6 = #ffca4c;
$color7 = #eeeeee;

$color8 = #007F2A;
$color9 = #7F0002;
$color10 = #00707F;
$color11 = #000E7F;

$font = 'serif';
// $font = 'Adventure_';
// $font = arial;

html, body
	font-family: $font;

a
	cursor: pointer;
	text-decoration: none;

.modal-body
	background-color: $color5;

.modal-header, .modal-footer
	background-color: $color6;

.btn-success
	background-color: $color8;

.btn-danger
	background-color: $color9;

.btn-warning
	background-color: $color5;

.btn-info
	background-color: $color10;

.btn-primary
	background-color: $color11;
